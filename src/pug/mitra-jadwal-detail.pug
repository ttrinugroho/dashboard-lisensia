block modal
  +modal_with_close('modal-ubah-jadwal','Form Ubah Jadwal')
    .p-4.d-flex.flex-column.gap-4
      div
        .d-inline-flex.align-items-center.gap-4.p-3.bg-light.rounded-2
          .subtitle Mode Manual
          .form-switch.body-super
            input#flexSwitchCheckChecked.form-check-input(type='checkbox' role='switch' checked='')
      .datepicker
        .action-item
          .action-head.w-100
            label.form-label(for='mulai') Tanggal Mulai
            input.form-control.js-date-range(type="text" data-single-month="true" data-container=".js-date-container" value="12 Agustus 2023")
          .action-body.w-100
            .js-date-container
      div
        label.form-label(for='proses') Lama Proses (Hari)
        input#proses.form-control(type="text")
      div
        label.form-label(for='selesai') Tanggal Selesai
        input#selesai.form-control(type="text" disabled value="25-08-2023")
  +footer_back
    button.btn.btn-light.px-8(type="button") Cancel
    button.btn.btn-primary.ms-4.px-8(type="button") Save
