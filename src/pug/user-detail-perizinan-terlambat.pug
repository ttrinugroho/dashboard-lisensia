                p.body.mb-2 Dokumen Master Pemohon
                .d-flex.align-items-center.gap-3
                  .flex-grow-1
                    .progress(role='progressbar' aria-label='Example 1px high' aria-valuenow=val.vn aria-valuemin='0' aria-valuemax='100' style='height: 4px')
                      .progress-bar(style={width: val.vn + '%'}, class='bg-'+val.bg)
                  small=val.vn + '%'
        .col
          .p-4.h-100.border.rounded
            each val in [{bg:'success',vn:'85'},{bg:'warning',vn:'48'},{bg:'warning',vn:'48'},{bg:'success',vn:'85'},{bg:'warning',vn:'48'},{bg:'danger',vn:'17'},{bg:'danger',vn:'17'}]
