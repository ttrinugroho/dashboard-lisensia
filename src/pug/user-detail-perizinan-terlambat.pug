block content
  .pb-5.mb-6
    .container.py-5.mb-5
      .row.row-cols-2.mb-4
        .col
          .p-4.h-100.border.rounded
            each val in [{bg:'success',vn:'85'},{bg:'warning',vn:'48'},{bg:'warning',vn:'48'},{bg:'success',vn:'85'},{bg:'warning',vn:'48'},{bg:'danger',vn:'17'},{bg:'danger',vn:'17'}]
