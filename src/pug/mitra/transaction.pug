                .d-flex.gap-4 
                  div
                    .form-check.body-super
                      input.form-check-input(type='radio' value='')
                  .py-7.px-5.border-top.border-5.border-primary.shadow-sm
                    .d-flex.gap-3.flex-column
                      h4.fw-bold.mb-0 IDR 750K
                        span.body.fw-semibold
                          small / Month
                      .d-flex.gap-4.align-items-center
                        small.fw-semibold.text-uppercase MONTHLY BUSINESS
